Package: pkgload
Title: Simulate Package Installation and Attach
Version: 0.0.0.9000
Authors@R: c(
    person("Hadley", "Wickham", , "hadley@rstudio.com", role = c("aut", "cre")),
    person("RStudio", role = "cph")
